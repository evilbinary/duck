import os

Import('env')

arch_type= env['ARCHTYPE']

source=[]
source+=Glob(arch_type+'/*.c')

include=[
    '.',
    "%s"""%(env.get('ARCHTYPE')),
    '../',
    '../include',
    '../../',
    '../include/archcommon',
]

env.Library('libarchcommon.a',source,CPPPATH=include,CFLAGS='$CFLAGS')


 